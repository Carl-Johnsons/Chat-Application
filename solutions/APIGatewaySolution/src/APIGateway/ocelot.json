{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "auth-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/Auth/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 1,
        "Limit": 1
      }
    },
    {
      "DownstreamHostAndPorts": [
        {
          "Host": "upload-api",
          "Port": 80
        }
      ],
      "DownstreamPathTemplate": "/api/Upload/{everything}",
      "DownstreamScheme": "http",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "UpstreamPathTemplate": "/api/Upload/{everything}"
    },
    {
      "DownstreamPathTemplate": "/chathub/{everything}",
      "DownstreamScheme": "ws",
      "DownstreamHostAndPorts": [
        {
          "Host": "websocket",
          "Port": 80
        }
      ],
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "UpstreamPathTemplate": "/chathub/{everything}"
    },
    {
      "DownstreamHostAndPorts": [
        {
          "Host": "conversation-api",
          "Port": 80
        }
      ],
      "DownstreamPathTemplate": "/api/Conversation/{everything}",
      "DownstreamScheme": "http",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "UpstreamPathTemplate": "/api/Conversation/{everything}"
    },
    {
      "DownstreamHostAndPorts": [
        {
          "Host": "conversation-api",
          "Port": 80
        }
      ],
      "DownstreamPathTemplate": "/api/Message/{everything}",
      "DownstreamScheme": "http",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "UpstreamPathTemplate": "/api/Conversation/Message/{everything}"
    },
    {
      "DownstreamHostAndPorts": [
        {
          "Host": "conversation-api",
          "Port": 80
        }
      ],
      "DownstreamPathTemplate": "/api/User/{everything}",
      "DownstreamScheme": "http",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "UpstreamPathTemplate": "/api/Conversation/User/{everything}"
    }
  ],

  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}