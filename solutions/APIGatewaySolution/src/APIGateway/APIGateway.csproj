		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<UserSecretsId>b649e23c-2992-418f-a64a-3421fd168585</UserSecretsId>
		<DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
